{
  "name": "gitsu",
  "version": "0.0.1",
  "description": "Util for quickly & easily switching git users from a config stored at ~/.gitsu",
  "author": "Jamie Weavis",
  "license": "ISC",
  "main": "./lib/src/app.js",
  "bin": {
    "gitsu": "./lib/src/app.js"
  },
  "keywords": [
    "git",
    "user",
    "switch",
    "gitsu"
  ],
  "scripts": {
    "start": "node ./lib/src/app.js",
    "dev": "ts-node-dev --respawn ./src/app.ts",
    "build": "tsc",
    "format": "prettier --write --ignore-unknown ./src",
    "lint": "eslint ./src"
  },
  "dependencies": {
    "@types/configstore": "^5.0.1",
    "@types/inquirer": "^8.2.1",
    "chalk": "^4.1.2",
    "commander": "^9.2.0",
    "configstore": "^6.0.0",
    "cosmiconfig": "^7.0.1",
    "inquirer": "^8.2.4",
    "log-symbols": "^4.1.0",
    "typescript": "^4.6.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "ts-node": "^10.8.0",
    "ts-node-dev": "^1.1.8"
  }
}
